/* pages/coupons/coupons.wxss */
.coupons-tab {
    overflow: hidden;
    background-color: #fff;
    display: flex;
    color: #7f8389;
}
.coupons-tab text {
    flex: 1;
    text-align: center;
    line-height: 100rpx;
    box-sizing: border-box;
    font-size: 30rpx;
}
.coupons-tab .on {
    color: #339900;
    border-bottom: 1rpx solid #339900;
}
.coupons-wrap {
    overflow: hidden;
    padding: 30rpx;
    margin: 0 -15rpx;
}
.coupons {
  width: 332rpx;
  height: 184rpx;
  margin: 0 15rpx 30rpx;
  float: left;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100% 100%;
  box-sizing: border-box;
  text-align: center;
  padding: 20rpx 0 40rpx 30rpx;
  display: block;
}

.coupons::after{border: none;}
.coupons .quota {
  color: #fff;
  line-height: 1;
  font-size: 36rpx;
}
.coupons .requirement {
  display: block;
  font-size: 32rpx;
  line-height: 30rpx;
  color: #e05d32;
}
