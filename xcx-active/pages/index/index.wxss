/* pages/index/index.wxss */
.coupons-tit {
  display: block;
  font-size: 26rpx;
  color: #a9a9a9;
  line-height: 1.5;
  background-color: #fff;
  text-align: left;
  font-weight: bold;
  padding: 30rpx;
}
.coupons-con {
  padding-left: 30rpx;
  white-space: nowrap;
  display: flex;
  background-color: #fff;
  padding-bottom: 40rpx;
  box-sizing: border-box;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.coupons {
  width: 248rpx;
  height: 141rpx;
  margin-right: 30rpx;
  display: inline-block;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100% 100%;
  padding: 20rpx 0 20rpx 30rpx;
  box-sizing: border-box;
  text-align: center;
}

.coupons text {
  color: #fff;
  line-height: 1.5;
  font-size: 18rpx;
}

.coupons .quota {
  font-size: 40rpx;
}

.coupons .requirement {
  display: block;
  font-size: 22rpx;
  color: #e05d32;
}

.active-wrap {
  width: 100%;
  overflow: hidden;
}

.active-tit {
  display: block;
  padding-left: 74rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #000;
  background: url(data:image/png;base64,) no-repeat 30rpx center;
  background-size: 34rpx 34rpx;
}

.active-list {
  overflow: hidden;
  background-color:#fff;
  padding-bottom: 20rpx;
  margin-bottom: 30rpx;
}

.active-list image {
  display: block;
  margin: 0 auto;
  width: 380rpx;
  height: 380rpx;
}

.product-tit {
  display: block;
  border-top: 1rpx solid #ddd;
  line-height: 54rpx;
  color: #000;
  padding: 32rpx 30rpx 0;
  font-size: 30rpx;
  overflow: hidden;
}

.product-pro {
  display: block;
  font-size: 26rpx;
  padding: 0 30rpx 0;
  color: #a9a9a9;
  line-height: 1.5;
}
.operation {float: left;}
.product-info {
  text-align: right;
  padding: 40rpx 30rpx 35rpx;
  color: #e4383b;
  font-size: 24rpx;
}

.specifications {
  float: left;
  color: #353535;
  font-size: 30rpx;
}

.price {
  font-size: 36rpx;
}

.operation-group {
  border-top: 1rpx solid #ddd;
  padding: 20rpx 30rpx 0;
  overflow: hidden;
  text-align: right;
}
.join-shoppingcar-btn,.purchase-btn {
  display: inline-block;
  font-size: 24rpx;
  color: #fff;
  background-color: #339900;
}
.purchase-btn {
  background-color: #e4383b;
  margin-left: 40rpx;
}
